{
  "name": "chii-docs",
  "version": "0.1.0",
  "description": "Documentation for Chii and Chobitsu",
  "main": "index.js",
  "scripts": {
    "dev": "vitepress dev docs",
    "init": "lsla shx cp node_modules/chii/public/target.js docs/public/target.js && lsla shx cp -r node_modules/chii/public/front_end docs/public/front_end",
    "build": "vitepress build docs",
    "preview": "vitepress preview docs",
    "format": "lsla prettier \"*.json\" \"docs/**/*.{mts,js,vue,css}\" --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liriliri/chii-docs.git"
  },
  "keywords": [
    "chii"
  ],
  "author": "surunzi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/liriliri/chii-docs/issues"
  },
  "homepage": "https://github.com/liriliri/chii-docs#readme",
  "devDependencies": {
    "@types/node": "^22.7.4",
    "chii": "^1.15.2",
    "licia": "^1.43.0",
    "luna-shader-toy-player": "^0.5.1",
    "vitepress": "^1.5.0"
  }
}
